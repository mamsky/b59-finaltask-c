<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <script src="https://cdn.tailwindcss.com"></script>
    <link rel="stylesheet" href="/assets/css/global.css">
    <link rel="icon" type="image/x-icon" href="/assets/image/bg-cat.png">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.7.2/css/all.min.css" integrity="sha512-Evv84Mr4kqVGRNSgIGL/F/aIDqQb7xQ2vcrdIwxfjThSH8CSR7PBEakCr51Ck+w+/U6swU2Im1vVX0SVk9ABhg==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

    <title>{{title}}</title>
  </head>
  <body>
    {{> navbar currenPage=title user=user }} 
    {{> @partial-block }}

    <script src="/js/helperDone.js"></script>
    <script src="/js/alert.js"></script>
    <script src="/js/alert.min"></script>
    {{#if messages.error}}
        <script>
            alertError('{{messages.error}}')  
        </script>
    {{/if}}
    {{#if messages.success}}
        <script>
            alertSuccess('{{messages.success}}')
        </script>
    {{/if}}
  </body>
</html>